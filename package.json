{
  "name": "wya-vm",
  "version": "0.0.9",
  "description": "",
  "main": "dist/vm.min.js",
  "files": [
    "src/**",
    "dist/**",
    "examples/**"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run all",
    "serve": "rm -rf build/ && cross-env NODE_ENV=development rollup --config rollup.config.js --watch",
    "demo": "cd examples/ && npm run dev",
    "build": "rm -rf dist/ && cross-env NODE_ENV=production rollup --config rollup.config.js && npm publish",
    "lint": "esw ./src  --ext .vue,.js",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --fix --watch",
    "all": "npm-run-all --parallel --race --continue-on-error lint:watch serve demo",
    "lint-staged": "lint-staged",
    "pre-commit:install": "cp .hooks/* .git/hooks/",
    "test": "jest --config ./tests/config/jest.config.js"
  },
  "author": "wya-team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wya-team/wya-vm"
  },
  "bugs": {
    "url": "https://github.com/wya-team/wya-vm/issues"
  },
  "dependencies": {
    "debug": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.7",
    "enzyme": "^3.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-vue": "^4.7.1",
    "eslint-watch": "^4.0.2",
    "fs-extra": "^7.0.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.5",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-nested": "^4.1.0",
    "postcss-simple-vars": "^5.0.1",
    "pre-commit": "^1.2.2",
    "prompt": "^1.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "rollup-watch": "^4.3.1",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "esw --fix",
      "git add"
    ],
    "*.md": [
      "esw --ext '.md' --fix",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ]
}
